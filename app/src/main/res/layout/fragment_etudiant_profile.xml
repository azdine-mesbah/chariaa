<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_content_Etudiant_profile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.Etudiant.Fragment_Etudiant_Profile">

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="false"
        android:layout_centerInParent="true"
        android:layout_centerHorizontal="true" />

    <ScrollView

        android:id="@+id/scrollView_etudiant_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:background="@color/overlay_light_20"
                android:src="@drawable/img"
                tools:visibility="gone" />

            <View
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textview_Nins"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="رقم التسجيل :"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txt_Nins"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <View

                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@color/overlay_dark_10" />

                <TextView
                    android:id="@+id/textview_AfullName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="الاسم الكامل - عربي :"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txt_Anom_Aprenom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <View

                    android:id="@+id/view5"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@color/overlay_dark_10" />

                <TextView
                    android:id="@+id/textview_Nom_Prenom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="الاسم الكامل - فرنسي :"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txt_Nom_Prenom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <View

                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@color/overlay_dark_10" />

                <TextView
                    android:id="@+id/textview_CNE"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="الرقم الوطني :"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txt_CNE"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@color/overlay_dark_10" />

                <TextView
                    android:id="@+id/textview_CIN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="رقم البطاقة الوطنية :"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txt_CIN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@color/overlay_dark_10" />

                <TextView
                    android:id="@+id/textview_dateNaiss"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="تاريخ الازدياد :"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txt_dateNaiss"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@color/overlay_dark_10" />

                <TextView
                    android:id="@+id/textview_lieuNaiss"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="مكان الازدياد :"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txt_lieuNaiss"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@color/overlay_dark_10" />

                <LinearLayout
                    android:id="@+id/View_Filiere"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/textview_Filiere"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="التخصص :"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/txt_Filiere"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:textAlignment="center"
                        android:textSize="16sp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="3dp"
                        android:layout_marginRight="3dp"
                        android:background="@color/overlay_dark_10" />

                </LinearLayout>

                <TextView
                    android:id="@+id/textview_Season1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="السداسية الاولى :"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txt_Season1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@color/overlay_dark_10" />

                <TextView
                    android:id="@+id/textview_Season2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="السداسية الثانية :"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txt_Season2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@color/overlay_dark_10" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>